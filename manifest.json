{
  "name": "aassd",
  "version": "1.2.2",
  "manifest_version": 2,
  "web_accessible_resources": ["*.hot-update.json", "test.js"],
  "permissions": ["tabs", "https://ya.ru/*"],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://ya.ru/*"]
    }
  ]
}
