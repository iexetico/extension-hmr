{
  "name": "aassd",
  "version": "1.2.2",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "web_accessible_resources": [
    {
      "resources": ["*.hot-update.json"],
      "matches": ["https://ya.ru/*"]
    }
  ],
  "host_permissions": ["https://ya.ru/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["./content.js"],
      "matches": ["https://ya.ru/*"]
    }
  ]
}
