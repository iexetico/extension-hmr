{
  "name": "aassd",
  "version": "1.2.2",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "web_accessible_resources": [
    {
      "resources": ["*.hot-update.json", "*.hot-update.js", "main-dom.js"],
      "matches": ["https://www.ea.com/*"]
    }
  ],
  "host_permissions": ["https://www.ea.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["./content.js"],
      "matches": ["https://www.ea.com/*"]
    }
  ]
}
